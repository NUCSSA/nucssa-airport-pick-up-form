{
  "JsonSchema": {
    "title": "司机报名表",
    "description": "",
    "type": "object",
    "required": [
      "name",
      "gender",
      "huskyEmail",
      "status",
      "degree",
      "wechatId",
      "phone",
      "carType",
      "availableTimeSlot"
    ],
    "properties": {
      "name": {
        "title": "姓名",
        "type": "string"
      },
      "gender": {
        "title": "性别",
        "type": "string",
        "enum": [
          "男",
          "女"
        ]
      },
      "huskyEmail": {
        "title": "Husky Email",
        "type": "string",
        "format": "email",
        "description": "必须为husky.neu.edu邮箱"
      },
      "email": {
        "title": "其他邮箱",
        "type": "string",
        "format": "email",
        "description": "最好为gmail"
      },
      "status": {
        "title": "就读项目",
        "type": "string",
        "enum": [
          "NEU在校生",
          "NEU毕业生"
        ]
      },
      "degree": {
        "title": "就读项目",
        "type": "string",
        "enum": [
          "本科",
          "硕士",
          "博士",
          "一毕业"
        ]
      },
      "wechatId": {
        "title": "微信ID",
        "type": "string",
        "description": "注意，不是昵称"
      },
      "phone": {
        "title": "电话",
        "type": "string",
        "description": "国内电话请加86"
      },
      "carType": {
        "title": "车型",
        "type": "string",
        "enum": [
          "2门轿车",
          "4门轿车",
          "SUV",
          "其他"
        ]
      },
      "availableTimeSlot": {
        "title": "8月份可接机时间",
        "type": "string",
        "enum": [
          "上午",
          "中午",
          "晚上",
          "全天"
        ]
      },
      "remark": {
        "title": "备注",
        "type": "string"
      }
    }
  },
  "UISchema": {
    "remark": {
      "ui:widget": "textarea"
    }
  }
}